export DEV_DOCKER=$(whoami)-dev
export OLD_JAVA_HOME=$JAVA_HOME
echo "OS: $OS"
if [ "$OS" == "Darwin" ]; then
    export JAVA_HOME="/usr/local/Cellar/openjdk@8/1.8.0+312/libexec/openjdk.jdk/Contents/Home"
elif [ "$OS" == "Linux" ]; then
    export JAVA_HOME="/usr/lib/jvm/java-8-openjdk-amd64"
else 
    echo "Unknown OS"
fi

echo "JAVA set to version $(javac -version)"
export OLD_LD_LIBRARY_PATH=$LD_LIBRARY_PATH
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:${WORKSPACE}/xiphos-spark-integration/xiphos-datasource/cpp/build:${WORKSPACE}/cpp/build/releases

export w=$WORKSPACE
alias b='$WORKSPACE/tools/build_gazelle_jni.sh -p'
alias t='$WORKSPACE/scripts/jni_shell < $WORKSPACE/tests/gazelle_jni_test.scala'
export SPARK_HOME=$WORKSPACE/spark-3.1.1-bin-hadoop2.7
export DAXL_CONFIG_FILE=${WORKSPACE}/xiphos-spark-integration/resources/config.yaml
alias s='stty sane'
